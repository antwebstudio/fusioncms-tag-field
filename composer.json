{
    "name": "antweb/fusioncms-tag-field",
    "description": "A FusionCMS addon.",
    "keywords": [
        "laravel",
        "fusioncms"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "spatie/laravel-tags": "*"
    },
    "autoload": {
        "psr-4": {
            "Addons\\TagFieldtype\\": "src/"
        }
    },
    "extra": {
        "fusioncms": {
            "name": "TagFieldtype"
        },
        "laravel": {
            "providers": [
                "Addons\\TagFieldtype\\Providers\\AddonServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
